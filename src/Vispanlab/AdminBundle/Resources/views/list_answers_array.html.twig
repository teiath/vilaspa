{% extends admin.getTemplate('base_list_field') %}

{% block field %}
<ul>
    {% for key, val in value %}
        <li>{% if val.is_correct is defined and val.is_correct == true %}[x] {% endif %}{{ val.answer|trans }}</li>
    {% endfor %}
</ul>
{% endblock %}
